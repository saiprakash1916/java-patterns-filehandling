import java.util.Scanner;
public class Pattern9 {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter how many lines");
		int n = sc.nextInt();
		sc.close();
		int rs = 1;
		for(int i=1;i<n;i++) {
			for(int j=0;j<i;j++){
					System.out.print(rs+" ");			
			}
			rs=rs+1;
			System.out.println();
		}
	}
}
