import java.util.Scanner;
public class Pattern4 {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter how many lines");
		int n = sc.nextInt();
		sc.close();
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=n;j++){
				if(j%2!=0)
				System.out.print(i);
			}
			System.out.println();
		}
	}
}
